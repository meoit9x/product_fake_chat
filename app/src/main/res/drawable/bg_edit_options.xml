<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="@color/color_F2F1FB"
                android:startColor="@color/color_F2F1FB" />
            <corners android:radius="@dimen/_8sdp" />
            <padding
                android:bottom="@dimen/_1sdp"
                android:left="@dimen/_1sdp"
                android:right="@dimen/_1sdp"
                android:top="@dimen/_1sdp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_F2F1FB" />
            <corners android:radius="@dimen/_8sdp" />
            <size
                android:width="@dimen/_40sdp"
                android:height="@dimen/_40sdp" />
        </shape>
    </item>
</layer-list>